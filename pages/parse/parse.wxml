<!--index.wxml-->
<view class="container">
  <view wx:for="{{content}}" wx:for-item="item" wx:key="*this">
    <text>{{item}}</text>
  </view>
  <view wx:for="{{button}}" wx:for-item="item" wx:key="*this">
    <button class="{{item.class}}" catchtap="onClick" data-id="{{item.href}}">{{item.content}}</button>
  </view>
</view>
